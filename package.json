{
    "name": "tu-huertita",
    "private": true,
    "version": "1.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "postbuild": "react-snap",
        "preview": "vite preview"
    },
    "reactSnap": {
        "source": "dist",
        "minifyHtml": {
            "collapseWhitespace": false,
            "removeComments": false
        },
        "puppeteerArgs": [
            "--no-sandbox",
            "--disable-setuid-sandbox"
        ],
        "skipThirdPartyRequests": true,
        "cacheAjaxRequests": false,
        "http2PushManifest": false,
        "crawl": true,
        "waitFor": 1000,
        "include": [
            "/",
            "/calendario",
            "/calculadora",
            "/recetas",
            "/blog/vermicompost",
            "/blog/huerta-balcon",
            "/blog/compost-departamento",
            "/blog/banco-semillas"
        ]
    },
    "dependencies": {
        "lucide-react": "^0.294.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-markdown": "^10.1.0",
        "react-router-dom": "^6.30.2",
        "rehype-raw": "^7.0.0"
    },
    "devDependencies": {
        "@types/react": "^18.2.37",
        "@types/react-dom": "^18.2.15",
        "@vitejs/plugin-react": "^4.2.0",
        "react-snap": "^1.23.0",
        "vite": "^5.0.0"
    }
}